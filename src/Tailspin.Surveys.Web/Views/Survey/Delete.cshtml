@model SurveyDTO
@{
    ViewData["Title"] = "Delete Survey";
}
<h2>@ViewData["Title"]</h2>

<h3 class="alert-danger">Are you sure you want to delete this survey?</h3>

@await Html.PartialAsync("_ValidationPartial")

<form asp-action="Delete" method="POST" asp-route-id="@Model.Id">
    @await Html.PartialAsync("_ConfirmPartial")
</form>

@await Html.PartialAsync("_SurveyDetailPartial", Model)


